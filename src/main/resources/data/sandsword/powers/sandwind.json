{
    "type": "origins:multiple",
    "velocity": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:add_velocity",
            "x": 0,
            "y": 0,
            "z": 1,
            "space": "local",
            "set": true
        },
        "interval": 1
    },
    "phase": {
        "type": "origins:phasing"
    },
    "kill_after_delay": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:delay",
            "ticks": 1,
            "action": {
                "type": "origins:execute_command",
                "command": "/kill @e[type=minecraft:armor_stand,limit=1,sort=nearest,tag=swind]",
                "permission_level": 4
            }
        },
        "interval": 20
    },
    "over_time": {
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:area_of_effect",
                    "radius": 4,
                    "bientity_action": {
                        "type": "origins:add_velocity",
                        "z": 1.5,
                        "y": 0.6,
                        "set": true,
                        "space": "world"
                    },
                    "bientity_condition": {
                        "type": "apoli:target_condition",
                        "condition": {
                            "type": "origins:origin",
                            "origin": "aridian:aridian",
                            "layer": "origins:origin",
                            "inverted": true
                        }
                    }
                },
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:falling_dust sand ~ ~ ~ 0.7 -1 0.7 3 20 force"
                },
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:sweep_attack ~ ~ ~ 0.7 -1 0.7 3 2 force"
                },
                {
                    "type": "apoli:execute_command",
                    "command": "playsound minecraft:entity.horse.breathe master IStarGame ~ ~ ~ 1 2 1"
                }
            ]
        }
    }
}